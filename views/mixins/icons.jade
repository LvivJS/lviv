mixin socialIcons(networks)
  each networkUrl, network in networks
    case network
      when "facebook"
        a.social_link(href="#{ networkUrl }", target="_blank")&attributes(attributes)
          +icon(network)
      when "twitter"
        a.social_link(href="#{ networkUrl }", target="_blank")&attributes(attributes)
          +icon(network)
      when "google-plus"
        a.social_link(href="#{ networkUrl }", target="_blank")&attributes(attributes)
          +icon(network)
      when "skype"
        a.social_link(href="#{ networkUrl }", target="_blank")&attributes(attributes)
          +icon(network)
        
